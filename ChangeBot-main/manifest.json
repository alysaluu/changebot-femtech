{
    "name": "ChangeBot",
    "description": "Send your petition to your local Representative/Senator",
    "version": "0.1.1",
    "manifest_version": 2,
    "icons": {
        "16": "Logos/finalLogo_icon.png"
    },
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.change.org/p/",
        "storage"
    ]
    
}